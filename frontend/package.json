{
    "name": "my-ads-platform-frontend",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^5.2.0",
        "@types/node": "^20.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "antd": "^5.12.0",
        "axios": "^1.6.0",
        "dayjs": "^1.11.0",
        "i18next": "^23.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.2.0",
        "react-i18next": "^13.5.0",
        "react-markdown": "^10.1.0",
        "react-router-dom": "^6.8.0",
        "react-scripts": "5.0.1",
        "recharts": "^2.8.0",
        "typescript": "4.9.5",
        "xlsx": "^0.18.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:coverage": "react-scripts test --coverage --watchAll=false",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^14.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.0.0"
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/build/"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/*.d.ts",
            "!src/index.tsx",
            "!src/reportWebVitals.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 70,
                "functions": 70,
                "lines": 70,
                "statements": 70
            }
        }
    }
}
